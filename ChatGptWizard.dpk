  designide,
  IndySystem,
  IndyProtocols,
  IndyCore,
  bindcomp,
  RESTComponents;

contains
  UChatGptMain in 'UChatGptMain.pas',
  UThread in 'UThread.pas',
  UQuestion in 'UQuestion.pas' {FrmChatGPT},
  UProgress in 'UProgress.pas' {Frm_Progress},
  UMenuHook in 'UMenuHook.pas';

end.
